## Defaults
IdentityFile ~/.ssh/id_rsa
IdentityFile ~/.ssh/tweek.pem

Host *
  ServerAliveInterval 60
  ForwardAgent yes

## TUNNELS
# ssh -f -N

Host giantdb.tunnel
  HostName 172.30.0.110
  User ubuntu
  IdentityFile ~/.ssh/tweek.pem
  LocalForward 6543 127.0.0.1:5432

