# Customs
#-------------------

# Commands
alias c="clear"

# Files
alias zshsource="source ~/.zshrc"
alias zshaliases="subl ~/.zprezto_aliases"
alias zshenv="subl ~/.zprezto_env"
alias zshopts="subl ~/.zprezto_opts"
alias zshconfig="subl ~/.zshrc"
alias zpreztoconfig="subl ~/.zpreztorc"

